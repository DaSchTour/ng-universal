{
  "name": "@nestjs-fixed/ng-universal",
  "version": "7.0.3",
  "description": "Nest - modern, fast, powerful node.js web framework (@ng-universal)",
  "author": "Kamil Mysliwiec",
  "license": "MIT",
  "scripts": {
    "test:schematics": "jest --run-in-band --silent",
    "build": "npm run build:lib && npm run build:schematics",
    "build:lib": "rollup --config rollup.config.ts --configPlugin typescript",
    "build:schematics": "tsc -p tsconfig.schematics.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "lint": "eslint lib/**/*.ts"
  },
  "peerDependencies": {
    "@angular/common": "^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "@angular/platform-server": "^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "@nestjs/common": "^9.0.0 || ^10.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0",
    "@nestjs/platform-express": "^9.0.0 || ^10.0.0",
    "@nguniversal/common": "^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "@nguniversal/express-engine": "^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "express": "^4.17.1",
    "zone.js": "^0.11.0 || ^0.12.0 || ^0.13.0"
  },
  "devDependencies": {
    "@angular/common": "15.1.4",
    "@angular/platform-server": "15.1.4",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-angular": "17.4.2",
    "@nestjs/common": "9.2.1",
    "@nestjs/core": "9.2.1",
    "@nguniversal/common": "15.1.0",
    "@nguniversal/express-engine": "15.0.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/jest": "29.2.3",
    "@types/memory-cache": "0.2.2",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "4.18.2",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "lint-staged": "13.0.3",
    "prettier": "2.8.8",
    "rollup-plugin-dts": "^4.2.2",
    "ts-jest": "29.0.3",
    "ts-morph": "16.0.0",
    "typescript": "4.9.5",
    "zone.js": "0.12.0"
  },
  "dependencies": {
    "@angular-devkit/schematics": "^15.0.0 || ^16.0.0",
    "@nguniversal/common": "^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "@nguniversal/express-engine": "^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "@schematics/angular": "^15.0.0 || ^16.0.0",
    "memory-cache": "0.2.0"
  },
  "schematics": "./schematics/collection.json",
  "lint-staged": {
    "*.ts": [
      "eslint ./lib --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "sideEffects": false,
  "fesm2020": "./dist/index.mjs",
  "fesm2015": "./dist/index.mjs",
  "esm2020": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "es2020": "./dist/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "esm2020": "./dist/index.mjs",
      "es2020": "./dist/index.mjs",
      "es2015": "./dist/index.mjs",
      "node": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": {
      "default": "./package.json"
    },
    "./tokens": {
      "types": "./dist/tokens.d.ts",
      "esm2020": "./dist/tokens.mjs",
      "es2020": "./dist/tokens.mjs",
      "es2015": "./dist/tokens.mjs",
      "node": "./dist/tokens.js",
      "default": "./dist/tokens.js"
    }
  }
}
