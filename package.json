{
  "name": "@nestjs-fixed/ng-universal",
  "version": "8.0.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@ng-universal)",
  "author": "Kamil Mysliwiec",
  "license": "MIT",
  "scripts": {
    "test:schematics": "jest --run-in-band --silent",
    "build": "npm run build:lib && npm run build:schematics",
    "build:lib": "rollup --config rollup.config.ts --configPlugin typescript",
    "build:schematics": "tsc -p tsconfig.schematics.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "lint": "eslint lib/**/*.ts"
  },
  "peerDependencies": {
    "@angular/common": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@angular/ssr": "^17.0.0",
    "@nestjs/common": "^9.0.0 || ^10.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0",
    "@nestjs/platform-express": "^9.0.0 || ^10.0.0",
    "express": "^4.17.1",
    "zone.js": "^0.14.0"
  },
  "devDependencies": {
    "@angular/cli": "17.0.5",
    "@angular/common": "17.0.5",
    "@angular/platform-server": "17.0.5",
    "@angular/ssr": "17.0.5",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-angular": "17.7.0",
    "@nestjs/common": "10.2.3",
    "@nestjs/core": "10.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "29.5.4",
    "@types/memory-cache": "0.2.3",
    "@types/node": "18.17.12",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^48.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "4.18.2",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "rollup": "^4.13.2",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "29.2.5",
    "ts-morph": "19.0.0",
    "typescript": "5.2.2",
    "zone.js": "0.14.2"
  },
  "dependencies": {
    "@angular-devkit/schematics": "17.0.5",
    "@angular/core": "17.0.5",
    "@schematics/angular": "17.0.5",
    "domino": "2.1.6",
    "memory-cache": "0.2.0"
  },
  "schematics": "./schematics/collection.json",
  "lint-staged": {
    "*.ts": [
      "eslint ./lib --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "sideEffects": false,
  "fesm2020": "./dist/index.mjs",
  "fesm2015": "./dist/index.mjs",
  "esm2020": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "es2020": "./dist/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "esm2020": "./dist/index.mjs",
      "es2020": "./dist/index.mjs",
      "es2015": "./dist/index.mjs",
      "node": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": {
      "default": "./package.json"
    },
    "./tokens": {
      "types": "./dist/tokens.d.ts",
      "esm2020": "./dist/tokens.mjs",
      "es2020": "./dist/tokens.mjs",
      "es2015": "./dist/tokens.mjs",
      "node": "./dist/tokens.js",
      "default": "./dist/tokens.js"
    }
  }
}
